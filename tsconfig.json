{
    "compilerOptions": {
        "module": "commonjs",
        "target": "esnext",
        "jsx": "react",
        "lib": ["dom", "esnext"],
        "strict": true,
        "noEmit": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "noUnusedLocals": false,
        "allowJs": false,
        "noImplicitAny": true,
        "skipLibCheck": true,
        "types": [
            "./node_modules/user-agent-data-types"
        ]
    },
    "exclude": ["node_modules", "public", ".cache"]
}
